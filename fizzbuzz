#include <iostream>

/*for multiples of 3, print Fizz instead of the number and for the multiples of 5, print Buzz. For numbers which are multiples of both 3 and 5 print, FizzBuzz.*/

int main() {

for (int i=1; i<=100; i++) {

  if (i%3 == 0) {
    std::cout << "Fizz\n";
   }
  else if (i%5 == 0) {
    std::cout << "Buzz\n";
   }
  else if (i%15 == 0) {
    std::cout << "FizzBuzz\n";
   }
  else {
    std::cout << i << "\n";
   }
}
 return 0;
}
